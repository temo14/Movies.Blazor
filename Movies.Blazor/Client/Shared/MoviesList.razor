@inject IJSRuntime js

<GenericList List="Movies">
    <ElementTemplate>
        <IndividualMovie Movie="context" DeleteMovie="DeleteMovie" />
    </ElementTemplate>
</GenericList>

@code {
    [Parameter] public List<Movie> Movies { get; set; }

    private async void DeleteMovie(Movie movie)
    {
        await js.MyFunction($"Are you sure want to delete {movie.Title}");
        var confirm = await js.Confirm($"Are you sure want to delete {movie.Title}");
        if (confirm)
        {
            Movies.Remove(movie);
        } 
    }
}